{
  "root": true,
  "extends": ["next/core-web-vitals", "prettier"],
  "plugins": ["prettier", "@typescript-eslint"],
  "parser": "@typescript-eslint/parser",
  "parserOptions": {
    "ecmaVersion": 2023,
    "sourceType": "module"
  },
  "rules": {
    "prettier/prettier": "error"
  },
  "ignorePatterns": ["node_modules/", ".next/", "out/"]
}
